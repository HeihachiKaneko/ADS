package produtos;

public class Produto {
	
	
	private String id;
	private String nome;
	private String descricao;
	private float preco;
	private int quantidade;
	
	public void registrarProduto(String id, float preco, String descricao, String nome) {
        this.id = id;
        this.preco = preco;
        this.descricao = descricao;
        this.nome = nome;
        this.quantidade = 0;
    }
	
}
